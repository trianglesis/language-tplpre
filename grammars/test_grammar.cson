      'match': '\\(string.\\quoted\\.single\\)'
  'string_quoted_double':
      'name': 'string.quoted.double'
      'match': '\\(string.\\quoted\\.double\\)'
  'string_quoted_triple':
      'name': 'string.quoted.triple'
      'match': '\\(string.\\quoted\\.triple\\)'
  'string_quoted_other':
